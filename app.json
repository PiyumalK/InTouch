{
  "expo": {
    "name": "InTouch",
    "slug": "InTouch",
    "version": "1.0.0",
    "sdkVersion": "38.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.intouch.intouch",
      "versionCode": 1,
      "icon": "./assets/logo.png"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
